#include <stdio.h>

int main () {
    
    int num_1 = 1;
    int num_2 = 1;
    
    for (int i = 0;i < 9;i++) {
        
        for (int n = 0;n < 9;n++) {
            printf("%d*%d=%d ",num_1,num_2,num_1 * num_2);
            num_2++;
        }
        printf("\n");
        num_1++;
        num_2 = 1;
    }
    
    return 0;
}
